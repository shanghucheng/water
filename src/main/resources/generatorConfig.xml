<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 数据库驱动包路径 -->
    <classPathEntry location="C:\Users\shanghucheng\.m2\repository\mysql\mysql-connector-java\5.1.6\mysql-connector-java-5.1.6.jar" />

    <!--<properties resource="dev.properties"/>-->

    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <property name="mergeable" value="false"></property>
        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="tk.mybatis.mapper.common.Mapper"/>
        </plugin>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/water"
                        userId="root"
                        password="">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.water.entity" targetProject="src/main/java" />

        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources"/>

        <javaClientGenerator targetPackage="com.water.mapper" targetProject="src/main/java" type="XMLMAPPER"/>

        <table tableName="deliver_order" domainObjectName="DeliverOrder"></table>
        <table tableName="deliver_order_item" domainObjectName="DeliverOrderItem"></table>
        <table tableName="scan_goods" domainObjectName="ScanGoods"></table>
        <table tableName="sys_user" domainObjectName="SysUser"></table>

    </context>
</generatorConfiguration>
